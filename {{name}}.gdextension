[configuration]

entry_symbol = "{{name}}_library_init"
compatibility_minimum = "4.2"

#[icons]

#StateMachine = "res://bin/icons/StateMachine.svg"

[libraries]

macos.debug = "res://bin/lib{{lib_name}}.macos.template_debug.framework"
macos.release = "res://bin/lib{{lib_name}}.macos.template_release.framework"
windows.debug.x86_32 = "res://bin/lib{{lib_name}}.windows.template_debug.x86_32.dll"
